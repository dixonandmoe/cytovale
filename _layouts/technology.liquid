                <path d="M12.732761,15.6 C12.1820234,15.6 11.9113338,16.0205731 12.1212856,16.5229306 L15.6963436,25.0770694 C15.9093727,25.5867899 16.2578383,25.5794269 16.4677902,25.0770694 L20.0428481,16.5229306 C20.2558772,16.0132101 19.9855326,15.6 19.4313727,15.6 L12.732761,15.6 Z" id="Path-2-Copy-8" fill="#6677CC"></path>
            </g>
        </svg>
          <h1>Cytovale System in Diagnostics</h1>
      </div>
    </div>
  </section>

  <section class="technology-system__diagrams">
    <div class="technology-system__div">

    {% for system in page.techniques.system %}
      {% capture thecycle %}{% cycle 'even', 'odd' %}{% endcapture %}

      <div class="row">
        {% if thecycle == "even" %}
          <div class="col-md-push-2 col-md-10 col-xs-24 left-diagram">        
        {% else %}
          <div class="col-md-12 col-md-push-11 col-xs-24 right-diagram">
        {% endif %}
            <figure>
              <img src={{system.image}} alt="Diagnostic" />
              <figcaption>
                <h3>{{ system.title }}</h3>
                <p>{{ system.description }}</p>
              </figcaption>
            </figure>
          </div>
      </div>

    {% endfor %}


    </div>
  </section>

</section>


<section class="layout-cta cta">
  <div class="row">
    <div class="col-xs-24 col-sm-11 col-md-offset-1 col-md-12">
      <h2 class="blue">Want to Collaborate<span class="qmark">?</span></h2>
      <h2>Get in Touch to Work Together</h2>
    </div>
    <div class="col-xs-24 col-sm-10 col-md-10 text-right">
      <a class="cta-link" href="/contact">CONTACT US</a>
    </div>
  </div>
</section>

</main>

<script>
  document.body.style.background = "#191234";
</script>
